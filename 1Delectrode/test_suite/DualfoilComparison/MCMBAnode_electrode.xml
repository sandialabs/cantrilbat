
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 
  <phase dim="2" id="anode_surface">
    <elementArray datasrc="elements.xml"> Li E </elementArray>
    <speciesArray datasrc="#species_Li_electrode">
           li_site 
    </speciesArray>
  
    <state>
      <temperature units="K"> 300.0 </temperature>
      <coverages> li_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_AnodeElectrode"/>

    <transport model="None"/>
    <phaseArray>
        metal_Li_LiIon_electrons
        ECDMC_LiPF6 
        MCMB_Interstitials_anode
        Li(S)
     </phaseArray>
  </phase>

  <!-- species definitions     -->
  <speciesData id="species_Li_electrode">

   <!-- species li_site   -->
    <species name="li_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_AnodeElectrode">
    <!--
       Lumped reaction on the  negative electrode.
    -->
    <reaction reversible="yes" type="ButlerVolmer_NoActivityCoeffs" id="0001">
      <equation> Li_C6-bulk [=]  V_C6-bulk + Li+  + electron_Li_LiIon </equation>
      <rateCoeff type="ExchangeCurrentDensity" >
        <electrochem beta="0.5"/>
        <Arrhenius_ExchangeCurrentDensity>
          <!--  The answer for this is that it should be 28.279 at 298K. And the
                activation energy is E/R = 4000 K. dualfoil almost always uses 4000 K as the
                default activation energy.
            -->
           <A>1.89665E7</A>
           <b>0.0</b>
           <E units="kJ/mol"> 33.25788 </E>
        </Arrhenius_ExchangeCurrentDensity>
      </rateCoeff>
      <reactants> Li_C6-bulk:1.0 </reactants>
      <products> V_C6-bulk:1.0 Li+:1.0  electron_Li_LiIon:1.0  </products>
    </reaction>

    <reaction reversible="yes" type="surface" id="0001">
      <equation> Li_C6-bulk [=]  V_C6-bulk + Li(S) </equation>
        <rateCoeff>
        <Arrhenius>
          <!-- fake reaction with zero reaction rate -->
           <A> 0.0E-50 </A>
           <b> 0.0 </b>
           <E units="kJ/mol"> 33.25788 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants> Li_C6-bulk:1.0 </reactants>
      <products> V_C6-bulk:1.0 Li(S):1.0  </products>
    </reaction>


  </reactionData>

</ctml>
