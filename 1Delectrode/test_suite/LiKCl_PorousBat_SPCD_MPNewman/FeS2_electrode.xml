
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
  <!-- phase FeS2_surface
    -->
  <phase dim="2" id="FeS2_surface">
    <elementArray datasrc="elements.xml"> Li Si E </elementArray>
    <speciesArray datasrc="#species_FeS2_electrode"> fes2_site </speciesArray>
  
    <state>
      <temperature units="K"> 773.0 </temperature>
      <coverages> fes2_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_FeS2_Electrode"/>

    <transport model="None"/>
    <phaseArray> metal_Li_LiCl_electrons LiKCl_electrolyte FeS2_A(S) FeS2_B(S) </phaseArray>
  </phase>


 <!-- species definitions     -->
  <speciesData id="species_FeS2_electrode">


    <species name="fes2_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_FeS2_Electrode">

  <!--
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> 1/4 FeS2_A(S) +  Li+  + electron_Li_LiCl [=]   1/4 FeS2_B(S) </equation>
      <rateCoeff>
        <electrochem beta="0.5"/>
        <Arrhenius>
           <A>3.00E2</A>
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>    FeS2_A(S):0.25 Li+:1.0  electron_Li_LiCl:1.0  </reactants>
      <products> FeS2_B(S):0.25 </products>
    </reaction>


  </reactionData>

</ctml>
