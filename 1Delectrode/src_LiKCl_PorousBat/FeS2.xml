  <phase dim="3" id="FeS2(S)">
    <elementArray datasrc="elements.xml">
      Fe S
    </elementArray>
    <speciesArray datasrc="#species_FeS2(S)"> FeS2(S) </speciesArray>
    <thermo model="StoichSubstance">
      <density units="g/cm3"> 5.013581 </density>
    </thermo>
    <!-- As of 6/11/2012 these are mostly guesses not actual values-->
    <transport model="Solid">
      <thermalConductivity model="coeffs" units="W/m/K">
        <floatArray size="2">  1.0, 5.0e-4 </floatArray>
      </thermalConductivity>
      <electricalConductivity model="Arrhenius" units="">
         <A>260.</A>
         <b>0.0</b>
         <E units="kJ/kmol">17100.</E>        
      </electricalConductivity>
      <defectActivity model="Constant">
         1e-4
      </defectActivity>
      <defectDiffusivity model="Arrhenius">
           <A units="m2/s">1.0</A>
           <b>0.0</b>
           <E units="kJ/kmol">90000.</E>
      </defectDiffusivity>
    </transport>
    <kinetics model="none"/>
  </phase>

 <!-- species definitions     -->
  <speciesData id="species_FeS2(S)">

    <!-- species FeS2(S)   -->
    <!--      melting temperature = 1400 K -->
    <species name="FeS2(S)">
      <atomArray> Fe:1 S:2 </atomArray>
     <standardState  model="constant_incompressible">
        <!--   molar volume from Bernardi Newman -->
        <molarVolume units="m3/kmol"> 0.02393 </molarVolume>
      </standardState>
      <!-- using numbers based on Lueth memo -->
      <thermo>
        <Shomate Pref="1 bar" Tmax="1400.0" Tmin="298">
          <floatArray size="7">
             83.5823,  -29.2631,   41.1873,  -11.6740,   -1.4300, -200.9219,  153.0176
             <!-- pyrite Pelton 72.3870,    8.8501,    0.0007,         0,   -1.1428, -197.3525,  131.4492 -->
             <!-- pyrite JANAF  82.6560,  -24.7776,   36.2218,   -9.9591,   -1.4078, -193.9248,  151.8388-->
             <!-- maracasite NIST 83.5823,  -29.2631,   41.1873,  -11.6740,   -1.4300, -200.3006,  153.0176 -->
          </floatArray>
        </Shomate>
      </thermo>
      <!-- M = 119.975, then evaluated from molarVolume -->
      <density units="g/cm3"> 5.013581 </density>
    </species>

  </speciesData>

