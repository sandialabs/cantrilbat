
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase KLiCl(L)    -->
  <!--
         Ok, the eutectic is measured to be X_b = X_LiCl = 0.595
         and 355 C = 628.15
    -->
  <phase dim="3" id="LiKCl_Margules">
    <elementArray datasrc="elements.xml">
       Li K Cl
    </elementArray>
    <speciesArray datasrc="#species_MoltenSalt"> 
        LiCl(L) KCl(L)
    </speciesArray>
    <thermo model="Margules">
      <activityCoefficients model="Margules" TempModel="constant">
         <binaryNeutralSpeciesParameters speciesA="KCl(L)" speciesB="LiCl(L)">
            <excessEnthalpy model="poly_Xb" terms="2" units="J/kmol">
                  -17570.E3, -377.E3
            </excessEnthalpy>
            <excessEntropy  model="poly_Xb" terms="2" units="J/kmol/K">
                 -7.627E3, 4.958E3
            </excessEntropy>
            <!-- excess volume fit from Janz density data by Lueth -->
            <excessVolume_Enthalpy model="poly_Xb" terms="2" units="ml/mol">
                 12.662, -6.5863
            </excessVolume_Enthalpy>
            <excessVolume_Entropy  model="poly_Xb" terms="2" units="ml/mol/K">
                 0.010823, -0.0042265
            </excessVolume_Entropy>
          </binaryNeutralSpeciesParameters>
       </activityCoefficients>
    </thermo>
    <transport model="Liquid">
       <viscosity>
          <compositionDependence model="logMoleFractions">
             <interaction speciesA="LiCl(L)" speciesB="KCl(L)">
                <Eij units="J/kmol"> -1.0e3 </Eij>
                <Sij units="J/kmol/K"> 80.0e-5 </Sij>
             </interaction>
          </compositionDependence>          
       </viscosity>
       <thermalConductivity>
          <compositionDependence model="massFractions"/>
       </thermalConductivity>
       <hydrodynamicRadius>
          <compositionDependence model="none"/>
       </hydrodynamicRadius>
       <speciesDiffusivity>
          <!-- this value not verified: 
          see LiKCl_ecipmoltenSalt_trans.xml for single ion interactions -->
          <compositionDependence model="pairwiseInteraction">
             <interaction speciesA="LiCl(L)" speciesB="KCl(L)">
                <Dij units="m/s"> 1.0e-8 </Dij>
             </interaction>
          </compositionDependence>          
       </speciesDiffusivity>
    </transport>     
    <kinetics model="none"/>
    <state>
       <temperature> 725.0 </temperature>
       <pressure units="atm"> 1.0 </pressure>
       <moleFractions> LiCl(L):1.0  </moleFractions>
    </state>
  </phase>

 <!-- species definitions     -->
  <speciesData id="species_MoltenSalt">

    <species name="KCl(L)">
      <atomArray> K:1 Cl:1 </atomArray>
      <standardState  model="constant_incompressible"> 
         <!-- Need to get temperature depedent volumes from Janz -->
         <!-- V(KCl) = 27.318 + 0.020496 * T [ml/mol] -->
         <molarVolume> 0.0375761 </molarVolume>
      </standardState>
      <thermo>
         <Shomate Pref="1 bar" Tmax="2000.0" Tmin="700.0">
          <floatArray size="7">
           73.59698,  0.0,      0.0,
           0.0,       0.0,      -443.7341,
           175.7209
          </floatArray>
        </Shomate>
      </thermo>
      <transport>
        <hydrodynamicRadius model="Constant" units="A">
            2.000
        </hydrodynamicRadius>
        <viscosity model="Arrhenius">
           <!-- Janz, JPCRD, 17, supplement 2, 1988 -->
           <A>5.727E-5</A>
           <b>0.0</b>
           <E units="J/kmol">26273e3.</E>
        </viscosity>
        <thermalConductivity model="coeffs">
           <floatArray size="2">  0.5, -5.0e-5 </floatArray>
        </thermalConductivity>
      </transport> 
      <density units="g/cm3"> 1.984 </density>
    </species>

    <species name="LiCl(L)">
      <atomArray> Li:1 Cl:1 </atomArray>
      <standardState  model="constant_incompressible"> 
         <!-- Need to get temperature depedent volumes from Janz -->
         <!-- V(LiCl) = 20.467 + 0.0087415 * T [ml/mol] -->
         <molarVolume> 0.02048004 </molarVolume>
      </standardState>
      <thermo>
        <Shomate Pref="1 bar" Tmax="2000.0" Tmin="700.0">
          <floatArray size="7">
           73.18025, -9.047232, -0.316390,
           0.079587, 0.013594, -417.1314,
           157.6711
          </floatArray>
        </Shomate>
      </thermo>
      <transport>
        <hydrodynamicRadius model="Constant" units="A">
            1.000
        </hydrodynamicRadius>
        <viscosity model="Arrhenius">
           <!-- Janz, JPCRD, 17, supplement 2, 1988 -->
           <A>6.578e-5</A>
           <b>0.0</b>
           <E units="J/kmol">23788.e3</E>
        </viscosity>
        <thermalConductivity model="coeffs">
           <floatArray size="2">  0.6, -15.0e-5 </floatArray>
        </thermalConductivity>
      </transport> 
      <density units="g/cm3"> 2.07 </density>
    </species>

  </speciesData>

</ctml>
