
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
  <!-- phase FeS2_3_surface
    -->
  <phase dim="2" id="FeS2_3_surface">
    <elementArray datasrc="elements.xml"> Li Si E </elementArray>
    <speciesArray datasrc="#species_FeS2_3_electrode"> fes2_3_site </speciesArray>
  
    <state>
      <temperature units="K"> 773.0 </temperature>
      <coverages> fes2_3_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_FeS2_3_Electrode"/>

    <transport model="None"/>
    <phaseArray> metal_Li_LiCl_electrons LiKCl_electrolyte Li[2+x]Fe[1-x]S2(S) Fe[1-x]S(S) Li2S(S) Fe(S) </phaseArray>
  </phase>


 <!-- species definitions     -->
  <speciesData id="species_FeS2_3_electrode">


    <species name="fes2_3_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_FeS2_3_Electrode">

  <!--
    -->

    <reaction reversible="yes" type="surface" id="0001">
      <equation> 0.5 Li2FeS2(A) +   Li+  + electron_Li_LiCl [=] Li2S(S) + 0.5 Fe(S) </equation>
      <rateCoeff>
        <electrochem beta="0.5"/>
        <Arrhenius>
           <A>4.00E4</A>
           <b>0.0</b>
           <!-- <E units="kJ/mol"> 20.0 </E> -->
           <E units="kJ/mol"> 60.0 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>  Li2FeS2(A):0.5  Li+:1.0  electron_Li_LiCl:1.0  </reactants>
      <products> Li2S(S):1.0 Fe(S):0.5 </products>
    </reaction>
  

    <reaction reversible="yes" type="surface" id="0003">
      <equation> 0.5 FeS(B) + 0.5 VaS(B) + Li+  + electron_Li_LiCl  [=]  0.5 Li2FeS2(A) </equation>
      <rateCoeff>
        <electrochem beta="0.5"/>
        <Arrhenius>
           <!-- <A>1.0E14</A> -->
           <A>1.0E14</A>
           <b>0.0</b>
           <!-- <E units="kJ/mol"> 20.0 </E> -->
           <E units="kJ/mol"> 60.0 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>   FeS(B):0.5 VaS(B):0.5   Li+:1.0  electron_Li_LiCl:1.0  </reactants>
      <products> Li2FeS2(A):0.5   </products>
    </reaction>

   <reaction reversible="yes" type="surface" id="0003">
      <equation> 6 Li[7/3]Fe[2/3]S2(A) + 3 FeS(B) [=]  7 Li2FeS2(A) + VaS(B) </equation>
      <rateCoeff>
        <electrochem beta="0.5"/>
        <Arrhenius>
           <!-- <A>1.0E4</A> -->
           <A>1.0E1</A>
           <b>0.0</b>
           <!-- <E units="kJ/mol"> 20.0 </E> -->
           <E units="kJ/mol"> 60.0 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>   Li[7/3]Fe[2/3]S2(A):6.0  FeS(B):3.0  </reactants>
      <products> Li2FeS2(A):7.0  VaS(B):1.0  </products>
    </reaction>




  </reactionData>

</ctml>
