 <?xml version="1.0"?>
 <ctml>
   <validate reactions="yes" species="yes"/>
    
   <!-- phase Li13Si4(S)    -->
   <phase dim="3" id="Li13Si4(S)">
      <elementArray datasrc="elements.xml">
         Li  Si
      </elementArray>
      <speciesArray datasrc="#species_LiSi(S)"> Li13Si4(S) </speciesArray>
      <thermo model="StoichSubstance">
      <!-- density from Nesper, Prog Solid State Chem, 1990 -->
      <density units="g/cm3"> 1.24 </density> 
      <!-- molar volume = 269.2 A^3 from Nesper -->
    </thermo>
    <!-- As of 6/11/2012 these are mostly guesses not actual values-->
    <transport model="Solid">
       <thermalConductivity model="coeffs" units="W/m/K">
         <floatArray size="2">  1.0, 5.0e-4 </floatArray>
       </thermalConductivity>
       <electricalConductivity model="Arrhenius" units="">
          <A>100.</A>
          <b>0.0</b>
          <E units="J/kmol">10000.</E>        
       </electricalConductivity>
       <defectActivity model="Constant">
          1e-4
       </defectActivity>
       <defectDiffusivity model="Arrhenius">
            <A units="m2/s">1.0</A>
            <b>0.0</b>
            <E units="J/kmol">90000.</E>
       </defectDiffusivity>
     </transport>
     <kinetics model="none"/>
   </phase>
           
          
   <!-- species definitions     -->
   <speciesData id="species_LiSi(S)">
        
      <!-- species Li13Si4(S)   -->
