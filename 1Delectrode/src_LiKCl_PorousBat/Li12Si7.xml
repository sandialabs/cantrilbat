       <defectActivity model="Constant">
          1e-4
       </defectActivity>
       <defectDiffusivity model="Arrhenius">
            <A units="m2/s">1.0</A>
            <b>0.0</b>
            <E units="J/kmol">90000.</E>
       </defectDiffusivity>
     </transport>
     <kinetics model="none"/>
   </phase>
           
          
