all:
	cd ex_gri;				@MAKE@
	cd CO2_solubility_test; 		@MAKE@
	cd she_electrode;			@MAKE@
	cd sce_electrode;			@MAKE@
	cd multiPhase;				@MAKE@

test:
	- cd multiPhase; 			@MAKE@ test
	- cd she_electrode;                     @MAKE@ test 
	- cd sce_electrode;                     @MAKE@ test
	- cd ex_gri;                            @MAKE@ test
	- cd CO2_solubility_test; ./runtest

testd:
	- cd multiPhase;			@MAKE@ testd 
	- cd she_electrode;                     @MAKE@ testd
	- cd sce_electrode;                     @MAKE@ testd
	- cd ex_gri;                            @MAKE@ testd
	- cd CO2_solubility_test; ./runtestd

clean:
	cd multiPhase;         			@MAKE@ clean 
	cd she_electrode;                       @MAKE@ clean
	cd sce_electrode;                       @MAKE@ clean
	cd ex_gri;                     		@MAKE@ clean 
	cd CO2_solubility_test;        		@MAKE@ clean

rebaseline:
	cd multiPhase;  			@MAKE@ rebaseline 
	cd she_electrode;                       @MAKE@ rebaseline
	cd sce_electrode;                       @MAKE@ rebaseline
	cd ex_gri;                              @MAKE@ rebaseline
	cd CO2_solubility_test;   ../rebase.sh


install:

depends:
	cd ex_gri;                              @MAKE@ depends
	cd CO2_solubility_test;                 @MAKE@ depends

