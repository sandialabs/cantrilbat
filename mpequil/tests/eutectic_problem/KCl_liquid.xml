<?xml version="1.0"?>
<!--
    KCl modeling Based on NIST database 
    $Id: KCl_liquid.xml 5 2012-02-23 21:34:18Z hkmoffa $
  -->
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase KCl(L)    -->
  <phase dim="3" id="KCl(L)">
    <elementArray datasrc="elements.xml">
       K Cl
    </elementArray>
    <speciesArray datasrc="#species_KCl(L)"> KCl(L) </speciesArray>
    <thermo model="StoichSubstance">
      <density units="g/cm3">1.984</density>
    </thermo>
    <transport model="None"/>
    <kinetics model="none"/>
  </phase>

 <!-- species definitions     -->
  <speciesData id="species_KCl(L)">

    <species name="KCl(L)">
      <!--
         Thermo Data entered from NIST Webbook HKM
         Density is the solid KCl density obtained from the CRC.
         I have not found a source for the liquid KCl density yet.
       -->
      <atomArray> K:1 Cl:1 </atomArray>
      <thermo>
        <Shomate Pref="1 bar" Tmax="2000.0" Tmin="700.0">
          <floatArray size="7">
           73.59698,  0.0,      0.0,
           0.0,       0.0,      -443.7341,
           175.7209 
          </floatArray>
        </Shomate>
      </thermo>
      <density units="g/cm3"> 1.984 </density>
    </species>

  </speciesData>

</ctml>
