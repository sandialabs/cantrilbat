<?xml version="1.0"?>
<!--
    KNaCl modeling Based on EQ3 database (as best as I can represent it)
    $Id: KCl_Solid.xml 5 2012-02-23 21:34:18Z hkmoffa $
  -->
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase KCl(S)    -->
  <phase dim="3" id="KCl(S)_nist">
    <elementArray datasrc="elements.xml">
       K Cl
    </elementArray>
    <speciesArray datasrc="#species_KCl(S)"> KCl(S)_nist </speciesArray>
    <thermo model="StoichSubstance">
      <density units="g/cm3">1.984</density>
    </thermo>
    <transport model="None"/>
    <kinetics model="none"/>
  </phase>

 <!-- species definitions     -->
  <speciesData id="species_KCl(S)">

    <species name="KCl(S)_nist">
      <!--
         Thermo Data entered from NIST Webbook HKM
         Density obtained from CRC
         7/16/2007
       -->
      <atomArray> K:1 Cl:1 </atomArray>
      <thermo>
        <Shomate Pref="1 atm" Tmax="900.0" Tmin="250.0">
          <floatArray size="7">
           35.41597,  70.03472, -91.38233,
           52.52426,  0.153460, -449.1357,
           108.9982
          </floatArray>
        </Shomate>
        <Shomate Pref="1 atm" Tmax="1044.0" Tmin="900.0">
          <floatArray size="7">
          -717.3845,  1247.861, -708.5144,
           141.4435,  103.6712,  1.760084,
          -738.1329
          </floatArray>
        </Shomate>
      </thermo>
      <density units="g/cm3"> 1.984 </density>
    </species>

  </speciesData>

</ctml>
