##################################################################################
install:
	@INSTALL@ -d @cta_bindir@
	@INSTALL@ -c findRefState @cta_bindir@
	@INSTALL@ -d @cta_incdir@
	$(INSTALL_TSC) thermoUtilProg.h  @cta_incdir@/thermoUtilProg.h
	@INSTALL@ -d @cta_libdir@
	$(INSTALL_TSC) thermoUtilProg.o @cta_libdir@
	$(INSTALL_TSC) xmlSpeciesCVS @cta_bindir@
	$(INSTALL_TSC) xmlPitzerAC @cta_bindir@
	$(INSTALL_TSC) xmlPitzerAC_Theta @cta_bindir@

clean:
	$(RM) $(OBJ_TOT) $(BarinConversion_EXE) *.d .depends .dependsT

#############################################################################
#          INSTALL RULES
##############################################################################



ifeq ($(wildcard .depends), .depends)
include .depends
endif

