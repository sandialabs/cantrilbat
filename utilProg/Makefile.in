##########################################################################
#           Setup The Zuzax Directory locations
#########################################################################
#

# These links are to Cantera's install space
ZUZAX_INCROOT =  @ct_incroot@
INSTALL_TSC = @abs_top_builddir@/bin/install_tsc


#
# Bring in the Zuzax includes through the .mak file
#
include $(ZUZAX_INCROOT)/zuzax/Zuzax.mak

#
# Set up the Zuzax addition library
#
CTPADDS_DIR = ../ctpadds/src
CTPADDS_DEP = $(CTPADDS_DIR)/libctpadds.a
CTPADDS_LIB = -L$(CTPADDS_DIR) -lctpadds
#
# Set up the Zuzax Application Base directory
#
#CTAPPLBASE_DIR = ../Electrode/src
#CTAPPLBASE_DEP =   $(CTAPPLBASE_DIR)/libElectrode.a
#CTAPPLBASE_LIB = -L$(CTAPPLBASE_DIR) -lElectrode
#
# Utilsrc setup
#
UTILSRC_DIR =../util_src/src
UTILSRC_DEP = $(UTILSRC_DIR)/libutil_src.a
UTILSRC     = -L$(UTILSRC_DIR) -lutil_src

#########################################################################
# Set up the executable extension
########################################################################

TT=@EXE_EXT@
ifeq ($(TT),)
EXE_EXT=
else
EXE_EXT=$(TT)
endif

PURIFY=@PURIFY@

#############################################################################

PROGB=BarinConversion_LiOH
OBJ_TOT= $(PROGB).o BarinConversion_Ca.o BarinConversion_C.o  thermoUtilProg.o \
          BarinConversion_Cu.o findRefState.o O2_NASAVerification.o


BarinConversion_EXE=$(PROGB)$(EXE_EXT)

all:  $(BarinConversion_EXE)  .dependsT BarinConversion_Ca BarinConversion_CaO \
      BarinConversion_C BarinConversion_Cu findRefState O2_NASAVerification \
      xmlSpeciesCVS xmlPitzerAC xmlPitzerAC_Theta xmlPitzerAC_Lambda xmlPitzerAC_Psi \
      xmlRxnTable


############################################################################
# section to set up compiler rules
###########################################################################
#
# the C++ compiler
#
CXX = @CXX@
CXXFLAGS = @CXXFLAGS@
LCXX_FLAGS = @LCXX_FLAGS@
LCXX_END_LIBS = @LCXX_END_LIBS@
CXXLDR = $(CXX)
#
# C compiler
#
CC = @CC@
CFLAGS = @CXXFLAGS@

LDEFINES = 

INCLUDE_DIRS= $(ZUZAX_TOTAL_INCLUDES) -I$(UTILSRC_DIR) -I$(CTPADDS_DIR)  

#
# Both the C and C++ compiler predefines
#
CCFLAGS =  $(LDEFINES) $(INCLUDE_DIRS)
#
.cpp.o: 
	$(PURIFY) $(CXX) $(CXXFLAGS) $(CCFLAGS) -c $<


############################################################################
# section to set up depends
###########################################################################
DEPENDS = $(OBJ_TOT:.o=.d) 


.cpp.d:
	@CXX_DEPENDS@ $(CXXFLAGS) $(CCFLAGS) -c $*.cpp > $*.d


.dependsT: $(DEPENDS)
	cat *.d > .depends
	touch .dependsT

depends: 
	$(MAKE) .dependsT

$(OBJ_TOT): Makefile

$(DEPENDS): Makefile

##############################################################################

$(BarinConversion_EXE): $(PROGB).o  thermoUtilProg.o  \
         $(ZUZAX_CORE_LIBS_DEP)  $(UTILSRC_DEP)
	$(PURIFY) $(CXXLDR) $(LCXX_FLAGS) -o $(BarinConversion_EXE)  $(PROGB).o thermoUtilProg.o  \
	          $(CTPADDS_LIB)  \
                  $(UTILSRC) $(ZUZAX_TOTAL_LIBS) \
                  $(LIBF2C) $(LCXX_END_LIBS) 

BarinConversion_Ca: BarinConversion_Ca.o  thermoUtilProg.o\
         $(ZUZAX_CORE_LIBS_DEP)  $(UTILSRC_DEP)
	$(PURIFY) $(CXXLDR) $(LCXX_FLAGS) -o BarinConversion_Ca BarinConversion_Ca.o thermoUtilProg.o  \
