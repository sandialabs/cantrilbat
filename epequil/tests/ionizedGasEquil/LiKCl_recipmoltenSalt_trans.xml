
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase KLiCl(L)    -->
  <!--
         Ok, the eutectic is measured to be X_b = X_LiCl = 0.595
         and 355 C = 628.15
    -->
  <phase dim="3" id="MoltenSalt_electrolyte">
    <elementArray datasrc="elements.xml">
       Li K Cl
    </elementArray>
    <speciesArray datasrc="#species_MoltenSalt"> 
        Li+ K+ Cl- 
    </speciesArray>
    <thermo model="IonsFromNeutralMolecule">
      <standardConc model="constant_volume" />
        <neutralMoleculePhase datasrc="LiKCl_Margules_trans.xml#LiKCl_Margules" />
	<activityCoefficients model="IonFromNeutral" TempModel="constant">
	  <specialSpecies> Cl-  </specialSpecies>
	  <secondSpecialSpecies> Li+ </secondSpecialSpecies>
	  <scalingConvention model="constantIonValue">
	    <ion> Cl- </ion>
	    <value> 0.0 </value> 
	    <autoSatisfaction> 1 </autoSatisfaction>
          </scalingConvention>
	  
       </activityCoefficients>
    </thermo>
    <transport model="Liquid">
       <viscosity>
          <compositionDependence model="logMoleFractions">
             <interaction speciesA="Li+" speciesB="K+">
	         <!-- 
	           interactions are from speciesA = LiCl(L)
		   and speciesB = KCl(L).
                   -->
                <Hij> 
                   <floatArray size="3" units="kJ/mol">  -16.575, 40.759, -59.916 </floatArray> 
                </Hij>
                <Sij>                    
                   <floatArray size="3" units="J/mol/K">  -17.116, 53.418, -68.425 </floatArray> 
                </Sij>
             </interaction>
          </compositionDependence>          
       </viscosity>
       <ionConductivity>
          <compositionDependence model="logMoleFractions">
             <interaction speciesA="Li+" speciesB="K+">
	         <!-- 
	           interactions are from speciesA = LiCl(L)
		   and speciesB = KCl(L).
                   -->
                <Hij>
                   <floatArray size="3" units="kJ/mol"> -37.985, 25.487, -15.987 </floatArray>
                </Hij>
                <Sij>                    
                   <floatArray size="3" units="J/mol/K">  -23.798, 7.1290, 0.26278 </floatArray> 
                </Sij>
             </interaction>
          </compositionDependence>          
       </ionConductivity>
       <mobilityRatio>
          <compositionDependence>
             <K+:Li+ model="moleFractionsExpT"> 
                <interaction speciesA="Li+" speciesB="K+">
	            <!-- 
	              interactions are from speciesA = LiCl(L)
		      and speciesB = KCl(L).
                      -->
                   <Aij> 0.1898 </Aij>
                   <Bij units="1/K"> 0.00047619 </Bij>
                </interaction>
             </K+:Li+>
             <K+:Cl- model="moleFractionsExpT"> </K+:Cl->
             <Li+:Cl- model="moleFractionsExpT"> </Li+:Cl->
          </compositionDependence>
       </mobilityRatio>
       <selfDiffusion>
          <compositionDependence>
             <Li+ model="logMoleFractions"> </Li+>
             <K+ model="logMoleFractions"> </K+>
             <Cl- model="logMoleFractions"> </Cl->
          </compositionDependence>          
       </selfDiffusion>
       <thermalConductivity>
          <compositionDependence model="moleFractions"/>
             <interaction speciesA="Li+" speciesB="K+">
	         <!-- 
	           interactions are from speciesA = LiCl(L)
		   and speciesB = KCl(L).
                   -->
             </interaction>
       </thermalConductivity>
       <hydrodynamicRadius>
          <compositionDependence model="none"/>
       </hydrodynamicRadius>
       <speciesDiffusivity>
          <velocityBasis basis="Cl-">
          </velocityBasis>
          <compositionDependence model="stefanMaxwell_PPN">
             <interaction speciesA="Li+" speciesB="Cl-">
                <Dij units="m/s"> 0.25e-8 </Dij>
             </interaction>
             <interaction speciesA="K+" speciesB="Cl-">
                <Dij units="m/s"> 0.4e-8 </Dij>
             </interaction>
             <interaction speciesA="Li+" speciesB="K+">
                <Dij units="m/s"> 0.8e-8 </Dij>
             </interaction>
          </compositionDependence>          
       </speciesDiffusivity>
    </transport>     
    <kinetics model="none"/>
    <state>
      <temperature  units="K"> 725.0 </temperature>
       <pressure units="atm"> 1.0 </pressure>
       <moleFractions> Li+:0.5 Cl-:0.5 </moleFractions> 
    </state>
  </phase>


 <!-- species definitions     -->
  <speciesData id="species_MoltenSalt">


    <species name="K+">
      <atomArray> K:1 </atomArray>
      <charge> 1 </charge>
      <standardState model="IonFromNeutral" />
      <thermo model="IonFromNeutral">
        <neutralSpeciesMultipliers> 
            KCl(L):1
        </neutralSpeciesMultipliers>
      </thermo>
      <transport>
        <hydrodynamicRadius model="Constant" units="A">
            2.000
        </hydrodynamicRadius>
        <viscosity model="Arrhenius">
	   <!-- moles of K+ represent K+ and Cl- from KCl properties -->
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <!-- Lueth Molten Salt Transport Memo -->
           <A units="cP">0.07309</A>
           <b>0.0</b>
           <E units="kJ/mol">24.002</E>
        </viscosity>
        <ionConductivity model="Arrhenius">
	   <!-- moles of K+ represent K+ and Cl- from KCl properties -->
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <!-- Lueth Molten Salt Transport Memo -->
           <A units="1/ohm/cm">6.1404</A>
           <b>0.0</b>
           <E units="kJ/mol">8.9312</E>
        </ionConductivity>
        <mobilityRatio>
           <K+:Li+ model="exptemp">
   	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
               <floatArray size="2">  0.9179, 0.000476197 </floatArray>
           </K+:Li+>
           <K+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </K+:Cl->
           <Li+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Li+:Cl->
        </mobilityRatio>
        <selfDiffusion>
           <Li+ model="Arrhenius">
	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
              <A units="cm2/s">0.00172</A>
              <b>0.0</b>
              <E units="kJ/mol">28.3</E>
           </Li+>
           <K+ model="Arrhenius">
	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
              <A units="cm2/s">0.00171</A>
              <b>0.0</b>
              <E units="kJ/mol">28.5</E>
           </K+>
           <Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Cl->
        </selfDiffusion>
        <thermalConductivity model="coeffs">
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <floatArray size="2">  0.5, -5.0e-5 </floatArray>
        </thermalConductivity>
      </transport> 
    </species>

    <species name="Cl-">
      <atomArray> Cl:1 </atomArray>
      <charge> -1 </charge>
      <standardState model="IonFromNeutral" />
      <thermo model="IonFromNeutral">
        <specialSpecies/>
        <neutralSpeciesMultipliers> 
            LiCl(L):0
        </neutralSpeciesMultipliers>
      </thermo>
      <transport>
        <hydrodynamicRadius model="Constant" units="A">
            1.000
        </hydrodynamicRadius>
        <viscosity model="Constant"> 1.0
	   <!-- 
	       moles of Cl- represented by K+ and Li+ 
	       from KCl and LiCl properties 
	     -->
	   <mixtureWeighting> 0.0 </mixtureWeighting>
        </viscosity>
        <ionConductivity model="Constant"> 1.0
	   <!-- 
	       moles of Cl- represented by K+ and Li+ 
	       from KCl and LiCl properties 
	     -->
	   <mixtureWeighting> 0.0 </mixtureWeighting>
        </ionConductivity>
        <mobilityRatio>
           <K+:Li+ model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </K+:Li+>
           <K+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </K+:Cl->
           <Li+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Li+:Cl->
        </mobilityRatio>
        <selfDiffusion>
           <Li+ model="Constant"> 1.0
	      <mixtureWeighting> 0.0 </mixtureWeighting>
            </Li+>
           <K+ model="Constant"> 1.0
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </K+>
           <Cl- model="Constant"> 1.0
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Cl->
        </selfDiffusion>
        <thermalConductivity model="Constant"> 1.0
	   <!-- 
	       moles of Cl- represented by K+ and Li+ 
	       from KCl and LiCl properties 
	     -->
	   <mixtureWeighting> 0.0 </mixtureWeighting>
        </thermalConductivity>
      </transport> 
    </species>

    <species name="Li+">
      <atomArray> Li:1 </atomArray>
      <charge> 1 </charge>
      <standardState model="IonFromNeutral" />

      <thermo model="IonFromNeutral">
        <secondSpecialSpecies/>
        <neutralSpeciesMultipliers> 
            LiCl(L):1
        </neutralSpeciesMultipliers>
      </thermo>
      <transport>
        <hydrodynamicRadius model="Constant" units="A">
            1.000
        </hydrodynamicRadius>
        <viscosity model="Arrhenius">
	   <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <!-- Lueth Molten Salt Transport Memo -->
           <A units="cP">0.06104</A>
           <b>0.0</b>
           <E units="kJ/mol">24.384</E>
        </viscosity>
        <ionConductivity model="Arrhenius">
	   <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <!-- Lueth Molten Salt Transport Memo -->
           <A units="1/ohm/cm">13.743</A>
           <b>0.0</b>
           <E units="kJ/mol">6.488</E>
        </ionConductivity>
        <mobilityRatio>
           <K+:Li+ model="exptemp">
   	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
              <floatArray size="2">  0.4982, 0.00047619 </floatArray>
           </K+:Li+>
           <K+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </K+:Cl->
           <Li+:Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Li+:Cl->
        </mobilityRatio>
        <selfDiffusion>
           <Li+ model="Arrhenius">
	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
              <A units="cm2/s">0.00171</A>
              <b>0.0</b>
              <E units="kJ/mol">20.7</E>
           </Li+>
           <K+ model="Arrhenius">
	      <!-- moles of Li+ represent Li+ and Cl- from LiCl properties -->
	      <mixtureWeighting> 2.0 </mixtureWeighting>
              <!-- Lueth Molten Salt Transport Memo -->
              <A units="cm2/s">0.00121</A>
              <b>0.0</b>
              <E units="kJ/mol">21.3</E>
           </K+>
           <Cl- model="Constant"> 1.0 
	      <mixtureWeighting> 0.0 </mixtureWeighting>
           </Cl->
        </selfDiffusion>
        <thermalConductivity model="coeffs">
	   <mixtureWeighting> 2.0 </mixtureWeighting>
           <floatArray size="2">  0.6, -15.0e-5 </floatArray>
        </thermalConductivity>
      </transport> 
    </species>

  </speciesData>

</ctml>
