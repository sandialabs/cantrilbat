#/bin/sh
build_python=@BUILD_PYTHON@

all:
	cd src; $(MAKE)
	cd tests; $(MAKE)

test:
	cd tests; $(MAKE) -s test

testd:
	cd tests; $(MAKE) -s testd

clean:
	cd src; $(RM) .depends ; $(MAKE) clean
	cd tests;  $(MAKE) clean

depends:
	cd src; $(MAKE) depends
	cd tests; $(MAKE) depends

install:
	cd src; $(MAKE) install

rebaseline:
	cd tests; $(MAKE) rebaseline

# end of file

