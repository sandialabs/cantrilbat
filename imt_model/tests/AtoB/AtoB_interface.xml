
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
  <!-- AtoB_surface
    -->
  <phase dim="2" id="AtoB_surface">
    <elementArray datasrc="elements.xml"> Li Si E </elementArray>
    <speciesArray datasrc="#AtoB_data"> AB_site </speciesArray>
  
    <state>
      <temperature units="K"> 773.0 </temperature>
      <coverages> AB_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_AtoB"/>

    <transport model="None"/>
    <phaseArray> Asoln(l) Bsoln(l) </phaseArray>
  </phase>


 <!-- species definitions     -->
  <speciesData id="AtoB_data">

    <species name="AB_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_AtoB">

  <!--
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> ASolute [=] BSolute </equation>
      <rateCoeff>
        <Arrhenius>
           <A>3.00E5</A>
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>    ASolute:1.0  </reactants>
      <products>     BSolute:1.0 </products>
    </reaction>


  </reactionData>

</ctml>
