<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase TBP_liquid    -->
  <phase dim="3" id="TBP_liquid">
    <elementArray datasrc="elements.xml">
        O H C P N U dummy3
    </elementArray>
    <speciesArray datasrc="#species_TBP_liquid"> 
        TBP(org) (TBP)2(org) TBP*H2O(org) (TBP)2*(H2O)2(org) (TBP)3*(H2O)6(org) TBP*HNO3(org) (TBP)2*HNO3(org) (TBP)2*UO2(NO3)2(org) hexane
    </speciesArray>
    <thermo model="IdealSolidSolution">
       <standardConc model="molar_volume" />
    </thermo>
    <standardConc model="molar_volume" />
    <transport model="None"/>
   <reactionArray datasrc="#reaction_data"/>

    <kinetics model="AqueousKinetics"/>
   <state>
      <temperature units="K">300.0</temperature>
      <pressure units="Pa">101325.0</pressure>
      <!-- mole fraction determined after Nitsch & Van Schoor (1983) initial [TBP] of 1.1 moles/L -->
      <moleFractions>
             TBP(org):0.1255
             hexane:0.8745
     </moleFractions>
   </state>
  </phase>

