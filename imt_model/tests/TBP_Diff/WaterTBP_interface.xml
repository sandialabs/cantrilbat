
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
  <!-- WaterTBP_Surface
    -->
  <phase dim="2" id="WaterTBP_surface">
    <elementArray datasrc="elements.xml"> Li Si E </elementArray>
    <speciesArray datasrc="#AtoB_data"> AB_site </speciesArray>
  
    <state>
      <temperature units="K"> 773.0 </temperature>
      <coverages> AB_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_WaterTBP"/>

    <transport model="None"/>
    <phaseArray> HNO3_electrolyte TBP_liquid </phaseArray>
  </phase>


 <!-- species definitions     -->
  <speciesData id="AtoB_data">

    <species name="AB_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_WaterTBP">

  <!--  
        Aqueous Water crossing over to complex with TBP(org)
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> TBP*H2O(org) [=] H2O(L) + TBP(org)  </equation>
      <rateCoeff>
        <Arrhenius>
           <A>1.00E-1</A>
           <!-- <A>3.00E5</A> -->
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>     TBP*H2O(org):1.0   </reactants>
      <products>  H2O(L):1.0 TBP(org):1.0   </products>
    </reaction>

    <!--  
        TBP crossing over the interface
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> TBP(org) [=]  TBP(aq) </equation>
      <rateCoeff>
        <Arrhenius>
           <A>1.00E-2</A>
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants>  TBP(org):1.0  </reactants>
      <products>   TBP(aq):1.0 </products>
    </reaction>

    <!--  
        HNO3 crossing over to complex with TBP(org)
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> TBP*HNO3(org) [=]  NO3- + H+ + TBP(org)  </equation>
      <rateCoeff>
        <Arrhenius>
           <A>1.00E-1</A>
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants> TBP*HNO3(org):1.0 </reactants>
      <products>   NO3-:1.0  H+:1.0  TBP(org):1.0 </products>
    </reaction>

   <!--  
        HNO3 crossing over to complex with TBP(org)
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation>  (TBP)2*UO2(NO3)2(org) [=]  UO2++ + 2 NO3- + 2 TBP(org)  </equation>
      <rateCoeff>
        <Arrhenius>
           <A>1.00E-2</A>
           <b>0.0</b>
           <E units="kJ/mol"> 20.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants> (TBP)2*UO2(NO3)2(org):1.0 </reactants>
      <products>   UO2++:1.0 NO3-:2.0   TBP(org):2.0 </products>
    </reaction>





  </reactionData>

</ctml>
