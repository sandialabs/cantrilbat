<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase EC_DMC_PC    -->
  <!--
         Excess energies of mixing can be obtianed from Ding or Liu
    -->
  <phase dim="3" id="EC_DMC">
    <elementArray datasrc="elements.xml">
       C H O
    </elementArray>
    <speciesArray datasrc="#species_AlkyleneCarbonates"> 
        EC_C3H4O3(L)  
        EC_C3H4O3(Cr)
        EC_C3H4O3(g)
        DMC_C3H6O3(L)
        DMC_C3H6O3(g)
        PC_C4H6O3(L)
        PC_C4H6O3(g)
    </speciesArray>
    <speciesArray datasrc="nasa_gas.xml#species_data"> 
        C2H4
    </speciesArray>

    <thermo model="IdealGas">
    </thermo>
    <transport model="None">
    </transport>     
    <kinetics model="none"/>
    <state>
       <temperature> 298.15 </temperature>
       <pressure units="atm"> 1.0 </pressure>
       <moleFractions> 
       		EC_C3H4O3(Cr):0.0 
       		EC_C3H4O3(L):0.5 
       		EC_C3H4O3(g):0.0 
       		DMC_C3H6O3(L):0.5
       		DMC_C3H6O3(g):0.0
       	</moleFractions>
    </state>
  </phase>

