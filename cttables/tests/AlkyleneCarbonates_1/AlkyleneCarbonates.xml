    <species name="EC_C3H4O3(Cr)">
      <atomArray> C:3 H:4 O:3 </atomArray>
      <standardState  model="constant_incompressible"> 
         <molarVolume> 1 </molarVolume>
      </standardState>
      <thermo>
        <!-- thermo data from the following sources:
               Cp curve fits:
                 Ding, M. S. (2004). "Liquid-solid phase equilibria and 
                 thermodynamic modeling for binary organic carbonates." 
                 Journal of Chemical and Engineering Data 49(2): 276-282. 
               Enthalpy of formation (to obtain a6):
                 Verevkin, S. P., V. N. Emel'yanenko, et al. (2008). 
                 "Cyclic alkylene carbonates. Experiment and first principle 
                 calculations for prediction of thermochemical properties." 
                 Journal of Chemical Thermodynamics 40(9): 1428-1432.
               Entropy set arbitrarily.  Relative entropy for liquid from H_fusion/T = 43.69
          -->
         <Shomate Pref="1 atm" Tmax="311.0" Tmin="200.0">
          <floatArray size="7">
            -117.6069,   2.1214e+003,       -8.3486e+003,
            1.2994e+004,     0.0,       -595.9205,
           -468.4867
          </floatArray>
        </Shomate>
        <!--superheated solid temperature fit - above Tmelt -->
         <Shomate Pref="1 atm" Tmax="521.0" Tmin="311.0">
          <floatArray size="7">
            84.1530,  131.0540,      0.0,
           0.0,       0.0,      -615.6967,
           112.8268
          </floatArray>
        </Shomate>
      </thermo>
      <density units="g/cm3"> 1.3218 </density>
    </species>


    <species name="EC_C3H4O3(L)">
      <atomArray> C:3 H:4 O:3 </atomArray>
      <standardState  model="constant_incompressible"> 
         <molarVolume> 1 </molarVolume>
      </standardState>
      <thermo>
        <!-- thermo data from the following sources:
               Cp curve fits:
                 Ding, M. S. (2004). "Liquid-solid phase equilibria and 
                 thermodynamic modeling for binary organic carbonates." 
                 Journal of Chemical and Engineering Data 49(2): 276-282. 
               Enthalpy of formation (to obtain a6):
                 Verevkin, S. P., V. N. Emel'yanenko, et al. (2008). 
                 "Cyclic alkylene carbonates. Experiment and first principle 
                 calculations for prediction of thermochemical properties." 
                 Journal of Chemical Thermodynamics 40(9): 1428-1432.
               Enthalpy of formation computed from crystal phase and heat of vaporization  -571.78
               Entropy for crystal phase set arbitrarily.  
               Relative entropy for liquid from H_fusion/T = 43.69
          -->
         <Shomate Pref="1 atm" Tmax="521.0" Tmin="311.0">
          <floatArray size="7">
           93.7949,   131.0540,      0.0,
           0.0,       0.0,      -607.2968,
           160.6024
          </floatArray>
        </Shomate>
      </thermo>
      <!-- estimate density 10% less than solid phase -->
      <density units="g/cm3"> 1.202 </density>
    </species>



    <species name="EC_C3H4O3(g)">
      <atomArray> C:3 H:4 O:3 </atomArray>
