<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
 <!-- phase EC_DMC_PC    -->
  <!--
         Excess energies of mixing can be obtianed from Ding or Liu
    -->
  <phase dim="3" id="EC_DMC">
    <elementArray datasrc="elements.xml">
       C H O
    </elementArray>
    <speciesArray datasrc="#species_MoltenSalt"> 
        EC_C3H4O3(L) DMC_C3H6O3(L)
    </speciesArray>
    <thermo model="Margules">
      <activityCoefficients model="Margules" TempModel="constant">
         <binaryNeutralSpeciesParameters speciesA="EC_C3H4O3(L)" speciesB="DMC_C3H6O3(L)">
            <excessEnthalpy model="poly_Xb" terms="2" units="J/kmol">
                  -17570.E3, -377.E3
            </excessEnthalpy>
            <excessEntropy  model="poly_Xb" terms="2" units="J/kmol/K">
                 -7.627E3, 4.958E3
            </excessEntropy>
          </binaryNeutralSpeciesParameters>
       </activityCoefficients>
    </thermo>
    <transport model="Liquid">
       <viscosity>
          <compositionDependence model="logMoleFractions">
             <interaction speciesA="DMC_C3H6O3(L)" speciesB="EC_C3H4O3(L)">
                <Eij units="J/kmol"> -1.0e3 </Eij>
                <Sij units="J/kmol/K"> 80.0e-5 </Sij>
             </interaction>
          </compositionDependence>          
       </viscosity>
       <thermalConductivity>
          <compositionDependence model="massFractions"/>
       </thermalConductivity>
       <hydrodynamicRadius>
          <compositionDependence model="none"/>
       </hydrodynamicRadius>
       <speciesDiffusivity>
          <compositionDependence model="pairwiseInteraction">
             <interaction speciesA="DMC_C3H6O3(L)" speciesB="EC_C3H4O3(L)">
                <Dij units="m/s"> 1.0e-8 </Dij>
             </interaction>
          </compositionDependence>          
       </speciesDiffusivity>
    </transport>     
    <kinetics model="none"/>
    <state>
       <temperature> 298.15 </temperature>
       <pressure units="atm"> 1.0 </pressure>
       <moleFractions> 
       		EC_C3H4O3(L):0.5 
       		DMC_C3H6O3(L):0.5  
       	</moleFractions>
    </state>
  </phase>

 <!-- species definitions     -->
  <speciesData id="species_AlkyleneCarbonates">


    <!-- species O2    -->
    <species name="O2">
      <atomArray>O:2 </atomArray>
      <note>TPIS89</note>
      <thermo>
        <NASA Tmax="1000.0" Tmin="200.0" P0="100000.0">
           <floatArray name="coeffs" size="7">
             3.782456360E+00,  -2.996734160E-03,   9.847302010E-06,  -9.681295090E-09, 
             3.243728370E-12,  -1.063943560E+03,   3.657675730E+00</floatArray>
        </NASA>
        <NASA Tmax="3500.0" Tmin="1000.0" P0="100000.0">
           <floatArray name="coeffs" size="7">
             3.282537840E+00,   1.483087540E-03,  -7.579666690E-07,   2.094705550E-10, 
             -2.167177940E-14,  -1.088457720E+03,   5.453231290E+00</floatArray>
        </NASA>
      </thermo>
      <transport model="gas_transport">
        <string title="geometry">linear</string>
        <LJ_welldepth units="K">107.400</LJ_welldepth>
        <LJ_diameter units="A">3.460</LJ_diameter>
        <dipoleMoment units="Debye">0.000</dipoleMoment>
        <polarizability units="A3">1.600</polarizability>
        <rotRelax>3.800</rotRelax>
      </transport>
    </species>




    <species name="EC_C3H4O3(L)">
      <atomArray> C:3 H:4 O:3 </atomArray>
      <standardState  model="constant_incompressible"> 
         <molarVolume> 1 </molarVolume>
      </standardState>
      <thermo>
        <!-- thermo data from the following sources:
             Liu, Z. K. (2003). "Thermodynamic modeling of organic carbonates for 
             lithium batteries." Journal of the Electrochemical Society 150(3): A359-A365.
          -->
         <Shomate Pref="1 bar" Tmax="521.0" Tmin="311.0">
          <floatArray size="7">
           0.76166,  0.22121,      0.0,
           0.0,       0.0,      -605.99,
           47.931
          </floatArray>
        </Shomate>
      </thermo>
      <density units="g/cm3"> 1.3218 </density>
    </species>

    <species name="DMC_C3H6O3(L)">
      <atomArray> C:3 H:6 O:3 </atomArray>
      <standardState  model="constant_incompressible"> 
         <molarVolume> 1 </molarVolume>
      </standardState>
      <thermo>
        <Shomate Pref="1 bar" Tmax="2000.0" Tmin="700.0">
          <floatArray size="7">
           73.18025, -9.047232, -0.316390,
           0.079587, 0.013594, -417.1314,
           157.6711
          </floatArray>
        </Shomate>
      </thermo>
      <density units="g/cm3"> 1.2057 </density>
    </species>

  </speciesData>

</ctml>
