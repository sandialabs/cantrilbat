if test ! -f cttables ; then
  if test -f ../../src/cttables ; then
    ln -s ../../src/cttables .
  fi
fi
/bin/rm -rf out.txt diff.out
cttables  cttables.inp > out.txt
diff good_out.txt out.txt > diff.out
retnStat=$?
if [ $retnStat = "0" ]; then
echo "Successful diff comparison on `pwd`"
else
echo "Unsuccessful diff comparison on `pwd`"
fi
