
<?xml version="1.0"?>
<ctml>
  <validate reactions="yes" species="yes"/>
                   
  <!-- phase aluminum_surface   -->
  <phase dim="2" id="aluminum_surface">
    <elementArray datasrc="elements.xml"> Al E </elementArray>
    <speciesArray datasrc="#species_Alelectrode"> al_site </speciesArray>
  
    <state>
      <temperature units="K"> 300.0 </temperature>
      <coverages> al_site:1.0 </coverages>
    </state>
    <thermo model="Surface">
      <site_density units="mol/cm2"> 3e-09 </site_density>
    </thermo>
    <kinetics model="Interface"/>
    <reactionArray datasrc="#data_AlElectrode"/>

    <transport model="None"/>
    <phaseArray> metalElectrons  AlCl_electrolyte Al(S) </phaseArray>
  </phase>


 <!-- species definitions     -->
  <speciesData id="species_Alelectrode">


   <!-- species al_site   -->
    <species name="al_site">
      <atomArray>  </atomArray>
      <thermo>
        <const_cp Tmax="5000.0" Tmin="100.0">
           <t0 units="K">298.15</t0>
           <h0 units="kJ/mol">0.0</h0>
           <s0 units="J/mol/K">0.0</s0>
           <cp0 units="J/mol/K">0.0</cp0>
        </const_cp>
      </thermo>
    </species>

  </speciesData>

 <reactionData id="data_AlElectrode">

  <!--
       Lumped reaction on the Aluminum electrode
    -->
    <reaction reversible="yes" type="surface" id="0001">
      <equation> Al(S) [=] Al+++  + 3 electronSHE </equation>
      <rateCoeff>
        <electrochem beta="0.5"/>
        <Arrhenius>
           <A>1.11800E+3</A>
           <b>0.0</b>
           <E units="kJ/mol"> 40.000000 </E>
        </Arrhenius>
      </rateCoeff>
      <reactants> Al(S):1.0 </reactants>
      <products> Al+++:1.0  electronSHE:3.0  </products>
    </reaction>


  </reactionData>

</ctml>
