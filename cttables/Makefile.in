build_python=@BUILD_PYTHON@

all:
	cd src; @MAKE@

clean:
	cd src; $(RM) .depends ; @MAKE@ clean
	cd tests; @MAKE@ clean

depends:
	cd src; @MAKE@ depends

install:
	cd src; @MAKE@ install
	cd tests; @MAKE@ install

test:
	cd tests; @MAKE@ test

